{
  "appId": "eu.rentyo.rentyo",
  "appName": "Rentyo",
  "webDir": "public",
  "bundledWebRuntime": false,
  "server": {
    "hostname": "localhost",
    "androidScheme": "http"
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "SplashScreen": {
      "launchShowDuration": 3000,
      "launchAutoHide": false,
      "backgroundColor": "#076B6A",
      "androidScaleType": "CENTER_CROP",
      "showSpinner": false,
      "splashFullScreen": false,
      "splashImmersive": false
    },
    "BarcodeScanner": {
      "installForce": true
    },
    "StatusBar": {
      "style": "LIGHT",
      "overlay": false,
      "backgroundColor": "#076B6A"
    },
    "SocialLogin": { 
      "google": {
        "clientId": "656400083949-nuj4kg2rvofjjecmtcoo29191erlkula.apps.googleusercontent.com",
        "redirectUri": "com.googleusercontent.apps.656400083949-nuj4kg2rvofjjecmtcoo29191erlkula:/oauth2redirect",
        "scopes": [
          "profile",
          "email"
        ]
      }
    }
  },
  "cordova": {}
}